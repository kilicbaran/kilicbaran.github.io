{
    "name": "baran-kilic-website",
    "version": "0.0.1",
    "scripts": {
        "dev": "svelte-kit dev",
        "build": "svelte-kit build",
        "preview": "svelte-kit preview",
        "postbuild": "svelte-sitemap --domain https://kilicbaran.github.io --trailing-slashes true -i 'google*' -i 'blog/tag/**' -i '404*' ",
        "deploy": "cd build && git init && git branch -m main && git add -A && git commit -m 'deploy' && git push -f git@github.com:kilicbaran/kilicbaran.github.io.git main:gh-pages"
    },
    "devDependencies": {
        "@rvxlab/tailwind-plugin-ios-full-height": "^1.1.0",
        "@sveltejs/adapter-static": "^1.0.0-next.29",
        "@sveltejs/kit": "v1.0.0-next.304",
        "autoprefixer": "^10.4.4",
        "cssnano": "^5.1.7",
        "postcss": "^8.4.12",
        "svelte": "^3.46.6",
        "svelte-preprocess": "^4.10.4",
        "svelte-sitemap": "^2.5.0",
        "tailwindcss": "^3.0.23"
    },
    "type": "module"
}